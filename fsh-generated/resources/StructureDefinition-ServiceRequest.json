        "type": [
          {
            "code": "CodeableConcept"
          }
        ],
        "isSummary": true,
        "binding": {
          "strength": "example",
          "valueSet": "http://hl7.org/fhir/ValueSet/procedure-reason"
        }
      },
      {
        "id": "ServiceRequest.reasonReference",
