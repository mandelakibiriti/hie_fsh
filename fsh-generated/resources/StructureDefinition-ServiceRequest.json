        "min": 0,
        "max": "*",
        "type": [
          {
            "code": "CodeableConcept"
          }
        ],
        "isSummary": true,
        "binding": {
          "strength": "example",
          "valueSet": "http://hl7.org/fhir/ValueSet/servicerequest-category"
        }
